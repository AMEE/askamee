<%= params[:callback]+'(' if params[:callback] -%>{
	"categories" : [
		<%= @categories.map{|x| "\"#{x}\"".html_safe}.join(",\n").html_safe %>
	],
	"terms" : [ 
		<%= @terms.map{|x| "\"#{x}\"".html_safe}.join(",\n").html_safe %>
	],
	"quantities" : [ 
		<%= @quantities.map{|x| "\"#{x.to_s}\"".html_safe}.join(",\n").html_safe %>
	],
	"subqueries" : [
	<%= @categories.map{|x| "\"/detail.json?category=#{x}&terms=#{@terms.join(',')}&quantities=#{@quantities.map{|x| x.to_s}.join(',')}#{'&callback='+params[:callback] if params[:callback]}\"".html_safe}.join(",\n").html_safe %>
	]
}<%= ');' if params[:callback] -%>
